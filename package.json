{
  "name": "scc-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --mode=sandboxDebug --appIdSuffix=sandbox",
    "android:production": "react-native run-android --mode=productionDebug",
    "android:local": "node scripts/sync-env-local.js && ENVFILE=.env.local react-native run-android --mode=sandboxDebug --appIdSuffix=sandbox",
    "ios": "react-native run-ios --scheme 'StairCrusherClubSandbox'",
    "ios:production": "react-native run-ios --scheme 'StairCrusherClub'",
    "ios:local": "node scripts/sync-env-local.js && ENVFILE=.env.local react-native run-ios --scheme 'StairCrusherClubSandbox'",
    "start": "ENVFILE=.env.sandbox react-native start",
    "start:production": "ENVFILE=.env.production react-native start",
    "start:local": "node scripts/sync-env-local.js && ENVFILE=.env.local react-native start",
    "web": "ENVFILE=.env.sandbox webpack serve --config webpack.config.js --mode development",
    "web:production": "ENVFILE=.env.production webpack serve --config webpack.config.js --mode development",
    "web:local": "node scripts/sync-env-local.js && ENVFILE=.env.local webpack serve --config webpack.config.js --mode development",
    "web:build": "webpack --config webpack.config.js --mode production && node scripts/generate-og-pages.js",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern 'web/' --ignore-pattern 'web-dist/'",
    "codegen": "openapi-generator-cli generate --generator-key dev",
    "ota-deploy:sandbox": "env-cmd -f subprojects/scc-frontend-build-configurations/sandbox/.env.hot-updater hot-updater deploy",
    "ota-deploy:production": "env-cmd -f subprojects/scc-frontend-build-configurations/production/.env.hot-updater hot-updater deploy",
    "console:sandbox": "env-cmd -f subprojects/scc-frontend-build-configurations/sandbox/.env.hot-updater hot-updater console",
    "console:production": "env-cmd -f subprojects/scc-frontend-build-configurations/production/.env.hot-updater hot-updater console"
  },
  "codegenConfig": {
    "name": "AppSpec",
    "type": "components",
    "jsSrcsDir": "specs",
    "android": {
      "javaPackageName": "club.staircrusher"
    },
    "ios": {
      "componentProvider": {
        "SccMapView": "RNTSccMapView"
      }
    }
  },
